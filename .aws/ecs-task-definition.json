{
    "executionRoleArn": "ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "cal-itp-benefits-client",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "cpu": 0,
            "environment": [
            ],
            "healthCheck": {
                "retries": 2,
                "command": [
                    "CMD-SHELL",
                    "python bin/healthcheck.py || exit 1"
                ],
                "timeout": 60,
                "interval": 30,
                "startPeriod": 30
            },
            "secrets": [
                {
                    "valueFrom": "arn:aws:secretsmanager:<region>:<aws_account_id>:secret:<secret_name>",
                    "name": "DJANGO_INIT_JSON"
                }
            ],
            "essential": true
        }
    ],
    "placementConstraints": [
    ],
    "memory": "512",
    "family": "cal-itp",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "volumes": [
    ]
}